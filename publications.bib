  author = {Tekman, Mehmet and Batut, B{\'e}r{\'e}nice and Ostrovsky, Alexander and Antoniewski, Christophe and Clements, Dave and Ramirez, Fidel and Etherington, Graham J and Hotz, Hans-Rudolf and Scholtalbers, Jelle and Manning, Jonathan R and Bellenger, Lea and Doyle, Maria A and Heydarian, Mohammad and Huang, Ni and Soranzo, Nicola and Moreno, Pablo and Mautner, Stefan and Papatheodorou, Irene and Nekrutenko, Anton and Taylor, James and Blankenberg, Daniel and Backofen, Rolf and Gr{\"u}ning, Bj{\"o}rn},
