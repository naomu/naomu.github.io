@article{jansenvanvurenPhylodynamicAnalysisEbola2019,
  title = {Phylodynamic {{Analysis}} of {{Ebola Virus Disease Transmission}} in {{Sierra Leone}}},
  author = {{Jansen van Vuren}, Petrus and Ladner, Jason T. and Grobbelaar, Antoinette A. and Wiley, Michael R. and Lovett, Sean and Allam, Mushal and Ismail, Arshad and {le Roux}, Chantel and Weyer, Jacqueline and Moolla, Naazneen and Storm, Nadia and Kgaladi, Joe and {Sanchez-Lockhart}, Mariano and Conteh, Ousman and Palacios, Gustavo and Paweska, Janusz T.},
