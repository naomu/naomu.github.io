  title = {{{TGFB1}} Induces Fetal Reprogramming and Enhances Intestinal Regeneration},
  author = {Chen, Lei and Qiu, Xia and Dupre, Abigail and {Pellon-Cardenas}, Oscar and Fan, Xiaojiao and Xu, Xiaoting and Rout, Prateeksha and Walton, Katherine D. and Burclaff, Joseph and Zhang, Ruolan and Fang, Wenxin and Ofer, Rachel and Logerfo, Alexandra and Vemuri, Kiranmayi and Bandyopadhyay, Sheila and Wang, Jianming and Barbet, Gaetan and Wang, Yan and Gao, Nan and Perekatt, Ansu O. and Hu, Wenwei and Magness, Scott T. and Spence, Jason R. and Verzi, Michael P.},
