@article{tsuchidaExpressionREGFamily2017,
  title = {Expression of {{REG}} Family Genes in Human Inflammatory Bowel Diseases and Its Regulation},
  author = {Tsuchida, Chikatsugu and {Sakuramoto-Tsuchida}, Sumiyo and Taked, Maiko and {Itaya-Hironaka}, Asako and Yamauchi, Akiyo and Misu, Masayasu and Shobatake, Ryogo and Uchiyama, Tomoko and Makino, Mai and {Pujol-Autonell}, Irma and {Vives-Pi}, Marta and Ohbayashi, Chiho and Takasawa, Shin},
  year = {2017},
