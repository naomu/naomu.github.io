@article{fernandez-juarezDifferentialEffectsVarying2020,
  title = {Differential {{Effects}} of {{Varying Concentrations}} of {{Phosphorus}}, {{Iron}}, and {{Nitrogen}} in {{N2-Fixing Cyanobacteria}}},
  author = {{Fern{\'a}ndez-Ju{\'a}rez}, V{\'i}ctor and {Bennasar-Figueras}, Antoni and {Sureda-Gomila}, Antoni and {Ramis-Munar}, Guillem and Agawin, Nona S. R.},
  year = {2020},
  journal = {Frontiers in Microbiology},
  volume = {11},
  issn = {1664-302X},
  urldate = {2022-05-01},
  abstract = {Diazotrophs or N2-fixers are one of the most ecologically significant groups in marine ecosystems (pelagic and benthic). Inorganic phosphorus (PO43{\textendash}) and iron (Fe) can limit the growth and N2-fixing capacities of cyanobacteria. However, studies investigating co-limitation of these factors are lacking. Here, we added different concentrations of PO43{\textendash} and Fe in two cyanobacterial species whose relatives can be found in seagrass habitats: the unicellular Halothece sp. (PCC 7418) and the filamentous Fischerella muscicola (PCC 73103), grown under different nitrate (NO3{\textendash}) concentrations and under N2 as sole N source, respectively. Their growth, pigment content, N2-fixation rates, oxidative stress responses, and morphological and cellular changes were investigated. Our results show a serial limitation of NO3{\textendash} and PO43{\textendash} (with NO3{\textendash} as the primary limiting nutrient) for Halothece sp. Simultaneous co-limitation of PO43{\textendash} and Fe was found for both species tested, and high levels of Fe (especially when added with high PO43{\textendash} levels) inhibited the growth of Halothece sp. Nutrient limitation (PO43{\textendash}, Fe, and/or NO3{\textendash}) enhanced oxidative stress responses, morphological changes, and apoptosis. Furthermore, an extensive bio-informatic analysis describing the predicted Pho, Fur, and NtcA regulons (involved in the survival of cells to P, Fe, and N limitation) was made using the complete genome of Halothece sp. as a model, showing the potential of this strain to adapt to different nutrient regimes (P, Fe, or N).},
  file = {E:\Zotero\storage\CEGWVS22\Fernández-Juárez et al. - 2020 - Differential Effects of Varying Concentrations of .pdf}
}

