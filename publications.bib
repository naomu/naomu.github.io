@article{reesVitroChronicDamage2022,
  title = {An in~Vitro Chronic Damage Model Impairs Inflammatory and Regenerative Responses in Human Colonoid Monolayers},
  author = {Rees, William D. and Telkar, Nikita and Lin, David T. S. and Wong, May Q. and Poloni, Chad and Fathi, Ayda and Kobor, Michael and Zachos, Nicholas C. and Steiner, Theodore S.},
