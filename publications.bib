@article{patelTuningLowvoltageactivatedAcurrent2012,
  title = {Tuning Low-Voltage-Activated {{A-current}} for Silent Gain Modulation},
  author = {Patel, Ameera X. and Murphy, Naomi and Burdakov, Denis},
