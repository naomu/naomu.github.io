@article{jacobEbolaVirusDisease2020,
  title = {Ebola Virus Disease},
  author = {Jacob, Shevin T. and Crozier, Ian and Fischer, William A. and Hewlett, Angela and Kraft, Colleen S. and Vega, Marc-Antoine de La and Soka, Moses J. and Wahl, Victoria and Griffiths, Anthony and Bollinger, Laura and Kuhn, Jens H.},
