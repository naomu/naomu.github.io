  keywords = {adult,Adult,Crohn Disease,enteral nutrition,Failure to Thrive,Humans,Infant,inflammatory bowel disease,Inflammatory Bowel Diseases,Male,minerals/trace elements,Mutation,parenteral nutrition,{Receptors, Interleukin-10},vitamins,weight loss}
