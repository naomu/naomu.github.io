@article{hoLongitudinalSingleCellTranscriptomics2021,
  title = {Longitudinal {{Single-Cell Transcriptomics Reveals}} a {{Role}} for {{Serpina3n-Mediated Resolution}} of {{Inflammation}} in a {{Mouse Colitis Model}}},
  author = {Ho, Yen-Ting and Shimbo, Takashi and Wijaya, Edward and Kitayama, Tomomi and Takaki, Satoshi and Ikegami, Kentaro and Miyashita, Kazuya and Ouchi, Yuya and Takaki, Eiichi and Yamamoto, Ryoma and Kaneda, Yasufumi and Tamai, Katsuto},
