@article{andujarBeneficialEffectShikonin2012,
  title = {Beneficial {{Effect}} of {{Shikonin}} on {{Experimental Colitis Induced}} by {{Dextran Sulfate Sodium}} in {{Balb}}/{{C Mice}}},
  author = {And{\'u}jar, Isabel and R{\'i}os, Jos{\'e} Luis and Giner, Rosa Mar{\'i}a and Miguel Cerd{\'a}, Jos{\'e} and Recio, Mar{\'i}a del Carmen},
