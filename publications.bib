  keywords = {Ebolavirus,Genetic Variation,Genomics,{Hemorrhagic Fever, Ebola},Humans,Mutation,{RNA, Small Interfering},Viral Proteins},
  file = {E:\Zotero\storage\75R2V8E6\Kugelman et al_2015_Evaluation of the potential impact of Ebola virus genomic drift on the efficacy.pdf}
}

@article{kugelmanMonitoringEbolaVirus2015,
  title = {Monitoring of {{Ebola Virus Makona Evolution}} through {{Establishment}} of {{Advanced Genomic Capability}} in {{Liberia}}},
