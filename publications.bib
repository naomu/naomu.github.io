@article{minamideIRF2MaintainsStemness2020,
  title = {{{IRF2}} Maintains the Stemness of Colonic Stem Cells by Limiting Physiological Stress from Interferon},
  author = {Minamide, Kana and Sato, Taku and Nakanishi, Yusuke and Ohno, Hiroshi and Kato, Tamotsu and Asano, Jumpei and Ohteki, Toshiaki},
  year = {2020},
