@article{swingleyIntegratingMarkovClustering2008,
  title = {Integrating {{Markov Clustering}} and {{Molecular Phylogenetics}} to {{Reconstruct}} the {{Cyanobacterial Species Tree}} from {{Conserved Protein Families}}},
  author = {Swingley, Wesley D. and Blankenship, Robert E. and Raymond, Jason},
  year = {2008},
  month = apr,
  journal = {Molecular Biology and Evolution},
  volume = {25},
  number = {4},
  pages = {643--654},
  issn = {0737-4038},
  doi = {10.1093/molbev/msn034},
  urldate = {2022-04-26},
  abstract = {Attempts to classify living organisms by their physical characteristics are as old as biology itself. The advent of protein and DNA sequencing{\textemdash}most notably the use of 16S ribosomal RNA{\textemdash}defined a new level of classification that now forms our basic understanding of the history of life on earth. High-throughput sequencing currently provides DNA sequences at an unprecedented rate, not only providing a wealth of information but also posing considerable analytical challenges. Here we present comparative genomics{\textendash}based methods useful for automating evolutionary analysis between any number of species. As a practical example, we applied our method to the well-studied cyanobacterial lineage. The 24 cyanobacterial genomes compared here occupy a wide variety of environmental niches and play major roles in global carbon and nitrogen cycles. By integrating phylogenetic data inferred for upward of 1,000 protein-coding genes common to all or most cyanobacteria, we have reconstructed an evolutionary history of the phylum, establishing a framework for resolving key issues regarding the evolution of their metabolic and phenotypic diversity. Greater resolution on individual branches can be attained by telescoping inward to the larger set of conserved proteins between fewer taxa. The construction of all individual protein phylogenies allows for quantitative tree scoring, providing insight into the evolutionary history of each protein family as well as probing the limits of phylogenetic resolution. The tools incorporated here are fast, computationally tractable, and easily extendable to other phyla and provide a scaleable framework for contrasting and integrating the information present in thousands of protein-coding genes within related genomes.},
  file = {E\:\\Zotero\\storage\\A54WGKYQ\\Swingley et al. - 2008 - Integrating Markov Clustering and Molecular Phylog.pdf;E\:\\Zotero\\storage\\8HJBW6CP\\1271131.html}
}

@article{switonskiReviewChromosomePolymorphism2003,
