@misc{karo-atarDirectReprogrammingIntestinal2021,
  title = {Direct Reprogramming of the Intestinal Epithelium by Parasitic Helminths Subverts Type 2 Immunity},
  author = {{Karo-Atar}, Danielle and Ouladan, Shaida and Javkar, Tanvi and Joumier, Loick and Matheson, Macy K. and Merritt, Sydney and Westfall, Susan and Rochette, Annie and Gentile, Maria E. and Fontes, Ghislaine and Fonseca, Gregory J. and Parisien, Marc and Diatchenko, Luda and von Moltke, Jakob and Malleshaiah, Mohan and Gregorieff, Alex and King, Irah L.},
