@article{latyshevaEvolutionNitrogenFixation2012,
  title = {The Evolution of Nitrogen Fixation in Cyanobacteria},
  author = {Latysheva, Natasha and Junker, Vivien L. and Palmer, William J. and Codd, Geoffrey A. and Barker, Daniel},
