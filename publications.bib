  title = {{{SLR-superscaffolder}}: A de Novo Scaffolding Tool for Synthetic Long Reads Using a Top-to-Bottom Scheme},
  shorttitle = {{{SLR-superscaffolder}}},
  author = {Guo, Lidong and Xu, Mengyang and Wang, Wenchao and Gu, Shengqiang and Zhao, Xia and Chen, Fang and Wang, Ou and Xu, Xun and Seim, Inge and Fan, Guangyi and Deng, Li and Liu, Xin},
  year = {2021},
