  file = {E:\Zotero\storage\ZIJUYNND\Heuberger et al. - 2021 - High Yap and Mll1 promote a persistent regenerativ.pdf}
}

@article{heYAPTAZAre,
  title = {{{YAP}} and {{TAZ}} Are Transcriptional Co-Activators of {{AP-1}} Proteins and {{STAT3}} during Breast Cellular Transformation},
