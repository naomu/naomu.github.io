@article{poghosyanLossNeuropilin2Murine2022,
  title = {Loss of {{Neuropilin-2}} in {{Murine Mesenchymal-like Colon Cancer Organoids Causes Mesenchymal-to-Epithelial Transition}} and an {{Acquired Dependency}} on {{Insulin-Receptor Signaling}} and {{Autophagy}}},
  author = {Poghosyan, Susanna and Frenkel, Nicola and Lentzas, Aristeidis and Laoukili, Jamila and Rinkes, Inne Borel and Kranenburg, Onno and Hagendoorn, Jeroen},
  year = {2022},
  month = jan,
  journal = {Cancers},
  volume = {14},
  number = {3},
  pages = {671},
  publisher = {{Multidisciplinary Digital Publishing Institute}},
  issn = {2072-6694},
  doi = {10.3390/cancers14030671},
  urldate = {2022-04-05},
  abstract = {Neuropilin-2 (Nrp2), an important regulator of lymphangiogenesis and lymphatic metastasis, has been associated with progression in colorectal cancer (CRC). However, the tumor cell-intrinsic role of Nrp2 in cancer progression is incompletely understood. To address this question, we employed CRISPR-Cas9 technology to generate Nrp2-knockout organoids derived from murine CRC tumors with a mesenchymal phenotype. Transcriptome profiling and tumor tissue analysis showed that Nrp2 loss resulted in mesenchymal-to-epithelial transition (MET), which was accompanied with restored polarity and tight junction stabilization. Signaling pathway analysis revealed that Nrp2-knockout organoids acquire de novo dependency on insulin receptor (IR) signaling and autophagy as alternative survival mechanisms. Combined inhibition of IR signaling and autophagy prevented the stabilization of cell-cell junctions, reduced metabolic activity, and caused profound cell death in Nrp2-knockout organoids. Collectively, the data demonstrate a key role for Nrp2 in maintaining the aggressive phenotype and survival of tumor-derived CRC organoids. The identified connection between Nrp2, insulin receptor signaling and autophagy may guide the development of novel combination-treatment strategies for aggressive CRC.},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  langid = {english},
  keywords = {autophagy,colorectal cancer,insulin receptor,mesenchymal-to-epithelial transition (MET),Neuropilin-2,organoids},
  file = {E\:\\Zotero\\storage\\SJTN6AMQ\\Poghosyan et al. - 2022 - Loss of Neuropilin-2 in Murine Mesenchymal-like Co.pdf;E\:\\Zotero\\storage\\832AK843\\671.html}
}

