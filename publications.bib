  keywords = {Colitis,Serpina3n,Single Cell RNA-Sequencing,Stromal Cell},
  file = {E\:\\Zotero\\storage\\HTW9FRQY\\Ho et al. - 2021 - Longitudinal Single-Cell Transcriptomics Reveals a.pdf;E\:\\Zotero\\storage\\LM6IMJCI\\S2352345X21000758.html}
}

@article{hoMDR1DeficiencyImpairs2018,
  title = {{{MDR1}} Deficiency Impairs Mitochondrial Homeostasis and Promotes Intestinal Inflammation},
  author = {Ho, G.-T. and Aird, R. E. and Liu, B. and Boyapati, R. K. and Kennedy, N. A. and Dorward, D. A. and Noble, C. L. and Shimizu, T. and Carter, R. N. and Chew, E. T. S. and Morton, N. M. and Rossi, A. G. and Sartor, R. B. and Iredale, J. P. and Satsangi, J.},
