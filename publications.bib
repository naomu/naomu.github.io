}

@article{romera-hernandezYap1DrivenIntestinalRepair2020,
  title = {Yap1-{{Driven Intestinal Repair Is Controlled}} by {{Group}} 3 {{Innate Lymphoid Cells}}},
  author = {{Romera-Hern{\'a}ndez}, M{\'o}nica and {Aparicio-Domingo}, Patricia and Papazian, Natalie and Karrich, Julien J. and Cornelissen, Ferry and Hoogenboezem, Remco M. and Samsom, Janneke N. and Cupedo, Tom},
  year = {2020},
