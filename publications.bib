@article{heubergerHighYapMll12021,
  title = {High {{Yap}} and {{Mll1}} Promote a Persistent Regenerative Cell State Induced by {{Notch}} Signaling and Loss of P53},
  author = {Heuberger, Julian and Grinat, Johanna and Kosel, Frauke and Liu, Lichao and Kunz, S{\'e}verine and Vidal, Ramon Oliveira and Keil, Marlen and Haybaeck, Johannes and Robine, Sylvie and Louvard, Daniel and Regenbrecht, Christian and Sporbert, Anje and Sauer, Sascha and {von Eyss}, Bj{\"o}rn and Sigal, Michael and Birchmeier, Walter},
