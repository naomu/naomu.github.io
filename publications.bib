@article{munkholmReviewArticleIncidence2003,
  title = {Review Article: The Incidence and Prevalence of Colorectal Cancer in Inflammatory Bowel Disease},
  shorttitle = {Review Article},
  author = {Munkholm, P.},
  year = {2003},
  journal = {Alimentary Pharmacology \& Therapeutics},
  volume = {18},
  number = {s2},
  pages = {1--5},
  issn = {1365-2036},
  doi = {10.1046/j.1365-2036.18.s2.2.x},
  urldate = {2022-04-19},
  abstract = {Although colorectal cancer (CRC), complicating ulcerative colitis and Crohn's disease, only accounts for 1{\textendash}2\% of all cases of CRC in the general population, it is considered a serious complication of the disease and accounts for approximately 15\% of all deaths in inflammatory bowel disease (IBD) patients. The magnitude of the risk was found to differ, even in population-based studies. Recent figures suggest that the risk of colon cancer for people with IBD increases by 0.5{\textendash}1.0\% yearly, 8{\textendash}10 years after diagnosis. The magnitude of CRC risk increases with early age at IBD diagnosis, longer duration of symptoms, and extent of the disease, with pancolitis having a more severe inflammation burden and risk of the dysplasia-carcinoma cascade. Considering the chronic nature of the disease, it is remarkable that there is such a low incidence of CRC in some of the population-based studies, and possible explanations have to be investigated. One possible cancer-protective factor could be treatment with 5-aminosalicylic acid preparations (5-ASAs). Adenocarcinoma of the small bowel is extremely rare, compared with adenocarcinoma of the large bowel. Although only few small bowel cancers have been reported in Crohn's disease, the number was significantly increased in relation to the expected number.},
  langid = {english},
  file = {E\:\\Zotero\\storage\\B4QVRU75\\Munkholm - 2003 - Review article the incidence and prevalence of co.pdf;E\:\\Zotero\\storage\\F4CINVSR\\j.1365-2036.18.s2.2.html;E\:\\Zotero\\storage\\RQVZYAC4\\j.1365-2036.18.s2.2.html}
}

@article{murataAscl2DependentCellDedifferentiation2020,
  title = {Ascl2-{{Dependent Cell Dedifferentiation Drives Regeneration}} of {{Ablated Intestinal Stem Cells}}},
  author = {Murata, Kazutaka and Jadhav, Unmesh and Madha, Shariq and {van Es}, Johan and Dean, Justin and Cavazza, Alessia and Wucherpfennig, Kai and Michor, Franziska and Clevers, Hans and Shivdasani, Ramesh A.},
  year = {2020},
  month = mar,
  journal = {Cell Stem Cell},
  volume = {26},
  number = {3},
  pages = {377-390.e6},
  issn = {1934-5909},
  doi = {10.1016/j.stem.2019.12.011},
  urldate = {2022-04-25},
  abstract = {Ablation of LGR5+ intestinal stem cells (ISCs) is associated with rapid restoration of the ISC compartment. Different intestinal crypt populations dedifferentiate to provide new ISCs, but the transcriptional and signaling trajectories that guide this process are unclear, and a large body of work suggests that quiescent ``reserve'' ISCs contribute to regeneration. By timing the interval between LGR5+ lineage tracing and lethal injury, we show that ISC regeneration is explained nearly completely by dedifferentiation, with contributions from absorptive and secretory progenitors. The ISC-restricted transcription factor ASCL2 confers measurable competitive advantage to resting ISCs and is essential to restore the ISC compartment. Regenerating cells re-express Ascl2 days before Lgr5, and single-cell RNA sequencing (scRNA-seq) analyses reveal transcriptional paths underlying dedifferentiation. ASCL2 target genes include the interleukin-11 (IL-11) receptor Il11ra1, and recombinant IL-11 enhances crypt cell regenerative potential. These findings reveal cell dedifferentiation as the principal means for ISC restoration and highlight an ASCL2-regulated signal that enables this adaptive response.},
  langid = {english},
  keywords = {facultative stem cells,reserve stem cells,stem cell dedifferentiation},
  file = {E\:\\Zotero\\storage\\6N4M4G6G\\Murata et al. - 2020 - Ascl2-Dependent Cell Dedifferentiation Drives Rege.pdf;E\:\\Zotero\\storage\\6XGKBV48\\S1934590919305697.html;E\:\\Zotero\\storage\\CVZYQIIN\\S1934590919305697.html;E\:\\Zotero\\storage\\FBJ4F79A\\S1934590919305697.html}
}

