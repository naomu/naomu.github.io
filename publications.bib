  langid = {english},
  keywords = {Cardiology,Drug discovery,Dyslipidaemias,Immunotherapy,Systems analysis},
  file = {E:\Zotero\storage\9UCKVCTB\Amadori et al_2023_Systems immunology-based drug repurposing framework to target inflammation in.pdf}
}

