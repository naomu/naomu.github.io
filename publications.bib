  author = {Sol{\'e}, Laura and {Lobo-Jarne}, Teresa and {\'A}lvarez-Villanueva, Daniel and {Alonso-Mara{\~n}{\'o}n}, Josune and Guill{\'e}n, Yolanda and Guix, Marta and Sangrador, Irene and Rozal{\'e}n, Catalina and Vert, Anna and Barbachano, Antonio and Lop, Joan and Salido, Marta and Bellosillo, Beatriz and {Garc{\'i}a-Romero}, Raquel and Garrido, Marta and Gonz{\'a}lez, Jessica and {Mart{\'i}nez-Iniesta}, Mar{\'i}a and {L{\'o}pez-Arribillaga}, Erika and Salazar, Ram{\'o}n and Montagut, Clara and Torres, Ferr{\'a}n and Iglesias, Mar and {Celi{\`a}-Terrassa}, Toni and Mu{\~n}oz, Alberto and Villanueva, Alberto and Bigas, Anna and Espinosa, Llu{\'i}s},
  year = {2022},
  month = may,
  journal = {Nature Communications},
