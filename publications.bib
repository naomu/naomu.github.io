@article{grau-boveResistancePirimiphosmethylWest2021,
  title = {Resistance to Pirimiphos-Methyl in {{West African Anopheles}} Is Spreading via Duplication and Introgression of the {{Ace1}} Locus},
  author = {{Grau-Bov{\'e}}, Xavier and Lucas, Eric and Pipini, Dimitra and Rippon, Emily and Hof, Arj{\`e}n E. van `t and Constant, Edi and Dadzie, Samuel and {Egyir-Yawson}, Alexander and Essandoh, John and Chabi, Joseph and Djogb{\'e}nou, Luc and Harding, Nicholas J. and Miles, Alistair and Kwiatkowski, Dominic and Donnelly, Martin J. and Weetman, David and Consortium, The Anopheles gambiae 1000 Genomes},
  year = {2021},
