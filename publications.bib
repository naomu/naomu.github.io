@article{oharaAdaptiveDifferentiationPromotes2022,
  title = {Adaptive Differentiation Promotes Intestinal Villus Recovery},
  author = {Ohara, Takahiro E. and Colonna, Marco and Stappenbeck, Thaddeus S.},
  year = {2022},
