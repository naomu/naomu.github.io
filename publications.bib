  pmcid = {PMC7829634},
  pmid = {33491336},
  keywords = {annotation,Computational Biology,{Databases, Genetic},differential expression,Gene Expression Profiling,harmonization,Humans,Molecular Sequence Annotation,scRNA-seq,{Sequence Analysis, RNA},Single-Cell Analysis,Supervised Machine Learning,variational inference},
