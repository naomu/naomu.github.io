@article{glazkoExtractingStrongestSignals2017,
  title = {Extracting the {{Strongest Signals}} from {{Omics Data}}: {{Differentially Expressed Pathways}} and {{Beyond}}},
  shorttitle = {Extracting the {{Strongest Signals}} from {{Omics Data}}},
  author = {Glazko, Galina and Rahmatallah, Yasir and Zybailov, Boris and {Emmert-Streib}, Frank},
  year = {2017},
