@article{zhangInferenceHighresolutionTrajectories2021,
  title = {Inference of High-Resolution Trajectories in Single-Cell {{RNA-seq}} Data by Using {{RNA}} Velocity},
  author = {Zhang, Ziqi and Zhang, Xiuwei},
  year = {2021},
  month = oct,
  journal = {Cell Reports Methods},
  volume = {1},
  number = {6},
  pages = {100095},
  issn = {26672375},
  doi = {10.1016/j.crmeth.2021.100095},
  urldate = {2023-01-29},
  abstract = {Trajectory inference (TI) methods infer cell developmental trajectory from single-cell RNA sequencing data. Current TI methods can be categorized into those using RNA velocity information and those using only singlecell gene expression data. The latter type of methods are restricted to certain trajectory structures, and cannot determine cell developmental direction. Recently proposed TI methods using RNA velocity information have limited accuracy. We present CellPath, a method that infers cell trajectories by integrating singlecell gene expression and RNA velocity information. CellPath overcomes the restrictions of TI methods that do not use RNA velocity information: it can find multiple high-resolution trajectories without constraints on the trajectory structure, and can automatically detect the direction of each trajectory path. We evaluate CellPath on both real and simulated datasets and show that CellPath finds more accurate and detailed trajectories than the state-of-the-art TI methods using or not using RNA velocity information.},
  langid = {english},
  file = {E\:\\Zotero\\storage\\C9B65PQ6\\Zhang and Zhang - 2021 - Inference of high-resolution trajectories in singl.pdf;E\:\\Zotero\\storage\\EKVQCZYM\\Zhang and Zhang - 2021 - Inference of high-resolution trajectories in singl.pdf}
}

@article{zhangLncRNAMALAT1Potential2020,
  title = {{{LncRNA MALAT1}}: {{A}} Potential Therapeutic Target in {{DSSinduced}} Ulcerative Colitis Progression in Vitro},
  shorttitle = {{{LncRNA MALAT1}}},
  author = {Zhang, Baoqin and Li, Tiantian and Wang, Chao and Han, Jing and Wang, Baiqing and Sun, Guangbin},
  year = {2020},
  month = nov,
  journal = {Tropical Journal of Pharmaceutical Research},
  volume = {19},
  number = {9},
  pages = {1871--1877},
  issn = {1596-9827},
  doi = {10.4314/tjpr.v19i9.11},
  urldate = {2022-08-06},
  abstract = {Purpose: Ulcerative colitis is a severe disease affecting human health worldwide. Studies have shown that lncRNA MALAT1 has a significant correlation with breast, pancreatic, colon and liver cancers, but its effects on colitis is yet to be discovered. In this study, the potential role of lncRNA MALAT1 and the underlying molecular mechanism in DSS-induced colitis were investigated in vitro.Methods: Colorectal mucosal cell line FHC was induced with dextran sulphate sodium (DSS) to form an in vitro colitis model. Transfection procedure was employed to up- or down-regulate the expressions of lncRNA MALAT1 or miR-30c-5p in FHC cells. Cell viabilities were detected by CCK-8 assay. RT-qPCR was applied for evaluating gene expressions in normal FHC and DSS-induced FHC cell lines, while protein expression levels of target genes were examined by Western blot analysis. Starbase was used to predict the molecular interaction between MALAT1 and miR-30c-5p, while luciferase reporter assay was utilized to verify the binding sites between the two genes.Results: Expression of MALAT1 in the DSS-induced FHC cells was high with low cell viabilities, compared to the normal FHC cells. In the DSS-induced colitis-like FHC cells, overexpression of MALAT1 inhibited cell viabilities, while its downregulation promoted it. MiR-30c-5p directly targets MALAT1 and inhibited its expression in DSS-treated FHC cells. Upregulation of miR-30c-5p increased cell viabilities. Bcl-xL expression was inhibited by the up-regulation of MALAT1, while that of Bax was enhanced and the mimics of miR-30c-5p reversed these observations, suggesting that the enhancement of apoptosis promoted by oe-MALAT1 could be inhibited by miR-30c-5p. The interaction between MALAT1 and miR-30c-5p regulated NF-{$\kappa$}B/TGF-{$\beta$}/Wnt-{$\beta$}-catenin signaling pathway.Conclusion: Overexpression of MALAT1 led to inhibition of cell viability, while apoptosis and inflammation were promoted by targeting miR-30c-5p via NF-{$\kappa$}B/TGF-{$\beta$}/Wnt-{$\beta$}-catenin signaling pathway. These findings suggest MALAT1 as a therapeutic target for treating colitis. Keywords: Colitis, MALAT1, miR-30c-5p, NF-{$\kappa$}B/TGF-{$\beta$}/Wnt-{$\beta$}-catenin},
  copyright = {Copyright (c)},
  langid = {english},
  keywords = {Colitis,MALAT1,miR-30c-5p,NF-{$\kappa$}B/TGF-{$\beta$}/Wnt-{$\beta$}-catenin},
  file = {E:\Zotero\storage\MQ7NGLXK\Zhang et al_2020_LncRNA MALAT1.pdf}
}

@article{zhangSingleCellAnalysesInform2020,
  title = {Single-{{Cell Analyses Inform Mechanisms}} of {{Myeloid-Targeted Therapies}} in {{Colon Cancer}}},
  author = {Zhang, Lei and Li, Ziyi and Skrzypczynska, Katarzyna M. and Fang, Qiao and Zhang, Wei and O'Brien, Sarah A. and He, Yao and Wang, Lynn and Zhang, Qiming and Kim, Aeryon and Gao, Ranran and Orf, Jessica and Wang, Tao and Sawant, Deepali and Kang, Jiajinlong and Bhatt, Dev and Lu, Daniel and Li, Chi-Ming and Rapaport, Aaron S. and Perez, Kristy and Ye, Yingjiang and Wang, Shan and Hu, Xueda and Ren, Xianwen and Ouyang, Wenjun and Shen, Zhanlong and Egen, Jackson G. and Zhang, Zemin and Yu, Xin},
  year = {2020},
