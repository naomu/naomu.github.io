@article{vuRegulationEMTColorectal2017,
  title = {Regulation of {{EMT}} in {{Colorectal Cancer}}: {{A Culprit}} in {{Metastasis}}},
  shorttitle = {Regulation of {{EMT}} in {{Colorectal Cancer}}},
  author = {Vu, Trung and Datta, Pran K.},
  year = {2017},
  month = dec,
  journal = {Cancers},
  volume = {9},
  number = {12},
  pages = {171},
  issn = {2072-6694},
  doi = {10.3390/cancers9120171},
  urldate = {2022-08-22},
  abstract = {Epithelial to mesenchymal transition (EMT) is a process during which cells lose their epithelial characteristics, for instance cell polarity and cell{\textendash}cell contact, and gain mesenchymal properties, such as increased motility. In colorectal cancer (CRC), EMT is associated with an invasive or metastatic phenotype. In this review, we discuss recent studies exploring novel regulation mechanisms of EMT in CRC, including the identification of new CRC EMT regulators. Upregulation of inducers can promote EMT, leading to increased invasiveness and metastasis in CRC. These inducers can downregulate E-cadherin and upregulate N-cadherin and vimentin (VIM) through modulating EMT-related signaling pathways, for instance WNT/{$\beta$}-catenin and TGF-{$\beta$}, and EMT transcription factors, such as zinc finger E-box binding homeobox 1 (ZEB1) and ZEB2. In addition, several microRNAs (miRNAs), including members of the miR-34 and miR-200 families, are found to target mRNAs of EMT-transcription factors, for example ZEB1, ZEB2, or SNAIL. Downregulation of these miRNAs is associated with distant metastasis and advanced stage tumors. Furthermore, the role of EMT in circulating tumor cells (CTCs) is also discussed. Mesenchymal markers on the surface of EMT CTCs were found to be associated with metastasis and could serve as potential biomarkers for metastasis. Altogether, these studies indicate that EMT is orchestrated by a complicated network, involving regulators of different signaling pathways. Further studies are required to understand the mechanisms underlying EMT in CRC.},
  pmcid = {PMC5742819},
  pmid = {29258163},
  file = {E:\Zotero\storage\QJ27E5MB\Vu_Datta_2017_Regulation of EMT in Colorectal Cancer.pdf}
}

