@article{bahClinicalPresentationPatients2015,
  title = {Clinical Presentation of Patients with {{Ebola}} Virus Disease in {{Conakry}}, {{Guinea}}},
  author = {Bah, Elhadj Ibrahima and Lamah, Marie-Claire and Fletcher, Tom and Jacob, Shevin T. and {Brett-Major}, David M. and Sall, Amadou Alpha and Shindo, Nahoko and Fischer, William A. and Lamontagne, Francois and Saliou, Sow Mamadou and Bausch, Daniel G. and Moumi{\'e}, Barry and Jagatic, Tim and Sprecher, Armand and Lawler, James V. and Mayet, Thierry and Jacquerioz, Frederique A. and M{\'e}ndez Baggi, Mar{\'i}a F. and Vallenas, Constanza and Clement, Christophe and Mardel, Simon and Faye, Ousmane and Faye, Oumar and Soropogui, Bar{\'e} and Magassouba, Nfaly and Koivogui, Lamine and Pinto, Ruxandra and Fowler, Robert A.},
  year = {2015},
  month = jan,
  journal = {The New England Journal of Medicine},
  volume = {372},
  number = {1},
  pages = {40--47},
  issn = {1533-4406},
  doi = {10.1056/NEJMoa1411249},
  abstract = {BACKGROUND: In March 2014, the World Health Organization was notified of an outbreak of Zaire ebolavirus in a remote area of Guinea. The outbreak then spread to the capital, Conakry, and to neighboring countries and has subsequently become the largest epidemic of Ebola virus disease (EVD) to date. METHODS: From March 25 to April 26, 2014, we performed a study of all patients with laboratory-confirmed EVD in Conakry. Mortality was the primary outcome. Secondary outcomes included patient characteristics, complications, treatments, and comparisons between survivors and nonsurvivors. RESULTS: Of 80 patients who presented with symptoms, 37 had laboratory-confirmed EVD. Among confirmed cases, the median age was 38 years (interquartile range, 28 to 46), 24 patients (65\%) were men, and 14 (38\%) were health care workers; among the health care workers, nosocomial transmission was implicated in 12 patients (32\%). Patients with confirmed EVD presented to the hospital a median of 5 days (interquartile range, 3 to 7) after the onset of symptoms, most commonly with fever (in 84\% of the patients; mean temperature, 38.6{\textdegree}C), fatigue (in 65\%), diarrhea (in 62\%), and tachycardia (mean heart rate, {$>$}93 beats per minute). Of these patients, 28 (76\%) were treated with intravenous fluids and 37 (100\%) with antibiotics. Sixteen patients (43\%) died, with a median time from symptom onset to death of 8 days (interquartile range, 7 to 11). Patients who were 40 years of age or older, as compared with those under the age of 40 years, had a relative risk of death of 3.49 (95\% confidence interval, 1.42 to 8.59; P=0.007). CONCLUSIONS: Patients with EVD presented with evidence of dehydration associated with vomiting and severe diarrhea. Despite attempts at volume repletion, antimicrobial therapy, and limited laboratory services, the rate of death was 43\%.},
  langid = {english},
  pmid = {25372658},
  keywords = {Adult,Age Factors,Anti-Infective Agents,Dehydration,Diarrhea,Ebolavirus,Epidemics,Female,Fever,Fluid Therapy,Guinea,{Hemorrhagic Fever, Ebola},Humans,Kaplan-Meier Estimate,Male,Middle Aged,Risk,Survival Rate,Tachycardia,Vomiting},
