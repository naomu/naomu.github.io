@article{kadochProteomicBioinformaticAnalysis2013,
  title = {Proteomic and Bioinformatic Analysis of Mammalian {{SWI}}/{{SNF}} Complexes Identifies Extensive Roles in Human Malignancy},
  author = {Kadoch, Cigall and Hargreaves, Diana C. and Hodges, Courtney and Elias, Laura and Ho, Lena and Ranish, Jeff and Crabtree, Gerald R.},
  year = {2013},
  month = jun,
  journal = {Nature Genetics},
  volume = {45},
